config {
    type: "operations",
    hasOutput: true,
    name: "location_raw"
}

CREATE OR REPLACE EXTERNAL TABLE
  ${self()} ( location_id INT64,
    company_id INT64,
    location_name STRING,
    location_type STRING,
    city_id INT64 ) OPTIONS ( format = 'CSV',
    uris = ['gs://${constants.GCS.bucket}/${constants.GCS.rootPath}/location.csv'],
    skip_leading_rows = 1 );
